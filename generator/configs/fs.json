{
  "$schema": "https://raw.githubusercontent.com/kamontat/fthelper/main/generator/schema/fs.json",
  "fs": {
    "variables": {
      "template": ".templates",
      "freqtrade": ".freqtrade",
      "config": "configs",
      "userdata": "user_data",
      "strategy": "strategies"
    },
    "template": {
      "type": "directory",
      "mode": "single",
      "fullpath": "{{ .current }}/{{ .template }}"
    },
    "freqtrade": {
      "type": "directory",
      "mode": "single",
      "fullpath": "{{ .current }}/{{ .freqtrade }}"
    }
  }
}
